{
  "name": "@liqlab/client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typechain:generateRouter": "yarn typechain --target=ethers-v5 ./contracts/Router.sol/Router.json --out-dir=./contracts/Router.sol",
    "typechain:generatePool": "yarn typechain --target=ethers-v5 ./contracts/Pool721.sol/Pool721.json --out-dir=./contracts/Pool721.sol",
    "typechain:generateNFT": "yarn typechain --target=ethers-v5 ./contracts/SampleNFT.sol/SampleNFT.json --out-dir=./contracts/SampleNFT.sol"
  },
  "dependencies": {
    "@typechain/ethers-v5": "^10.1.0",
    "@types/react-modal": "^3.13.1",
    "@usedapp/core": "^1.1.5",
    "ethers": "^5.7.2",
    "moralis-v1": "^1.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-moralis": "^1.4.2",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "react-toastify": "^9.0.8",
    "react-use": "^17.4.0",
    "typechain": "^8.1.0",
    "vite-plugin-svgr": "^2.2.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
